# Database (replace with your actual staging MongoDB URI)
# IMPORTANT: You must provide the actual staging MongoDB URI in Railway variables
MONGODB_URI=${MONGODB_URI}

# Server
PORT=3000
NODE_ENV=staging

# JWT
JWT_SECRET=${JWT_SECRET}

# URLs
API_URL=${API_URL}
FRONTEND_URL=${FRONTEND_URL}

# Frontend URL Configuration (Railway handles routing)
FRONTEND_URL_LOCAL=${FRONTEND_URL}
FRONTEND_URL_NETWORK=${FRONTEND_URL}
BACKEND_URL_LOCAL=${API_URL}
BACKEND_URL_NETWORK=${API_URL}

# Email Configuration
SMTP_USER=${SMTP_USER}
SMTP_PASSWORD=${SMTP_PASSWORD}
SMTP_FROM_ADDRESS=${SMTP_FROM_ADDRESS}

# Admin
ADMIN_PASSWORD=${ADMIN_PASSWORD}

# Network Mode
NETWORK_MODE=false

# Prospect Notifications
PROSPECT_NOTIFICATION_RECIPIENTS=${PROSPECT_NOTIFICATION_RECIPIENTS} 